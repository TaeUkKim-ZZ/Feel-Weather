<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.feel.weather">
    <uses-sdk android:minSdkVersion="8" android:targetSdkVersion="19"/>

    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>

    <application android:allowBackup="true" android:icon="@drawable/cloud" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name="com.feel.weather.MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_add_friends" android:name="com.feel.weather.AddFriendsActivity" android:screenOrientation="portrait"/>
        <activity android:label="@string/title_activity_my_pro_file" android:name="com.feel.weather.MyProFileActivity" android:screenOrientation="portrait">
        </activity>
        <activity android:label="@string/title_activity_friends" android:name="com.feel.weather.FriendsActivity" android:screenOrientation="portrait">
        </activity>
        <activity android:label="@string/title_activity_add_user" android:name="com.feel.weather.AddUserActivity" android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>
